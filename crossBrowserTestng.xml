<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CrossBrowser Suite">
<listeners>
<listener class-name="dsAlgo_Utilities.Listeners"></listener>
		<listener class-name="com.aventstack.chaintest.plugins.ChainTestListener"/>
		<listener class-name="dsAlgo_Utilities.ExtentReporters"/>
</listeners>
    <test name="Edge Test">
        <parameter name="browserType" value="Edge" />
        <classes>
            <class name="dsAlgo_TestClasses.Array_TestClass" />
            <class name="dsAlgo_TestClasses.Tree_TestClass"/>
            <class name="dsAlgo_TestClasses.DataStructure_TestClass" />
            <class name="dsAlgo_TestClasses.Graph_TestClass"/>
            <class name="dsAlgo_TestClasses.Home_TestClass" />
            <class name="dsAlgo_TestClasses.LinkedList_TestClass"/>
            <class name="dsAlgo_TestClasses.Login_TestClass" />
            <class name="dsAlgo_TestClasses.Queue_TestClass"/>
            <class name="dsAlgo_TestClasses.Register_TestClass" />
            <class name="dsAlgo_TestClasses.Stack_TestClass"/>
        </classes>
    </test>
    <test name="Chrome Test">
        <parameter name="browserType" value="Chrome" />
        <classes>
            <class name="dsAlgo_TestClasses.Array_TestClass" />
            <class name="dsAlgo_TestClasses.Tree_TestClass"/>
            <class name="dsAlgo_TestClasses.DataStructure_TestClass" />
            <class name="dsAlgo_TestClasses.Graph_TestClass"/>
            <class name="dsAlgo_TestClasses.Home_TestClass" />
            <class name="dsAlgo_TestClasses.LinkedList_TestClass"/>
            <class name="dsAlgo_TestClasses.Login_TestClass" />
            <class name="dsAlgo_TestClasses.Queue_TestClass"/>
            <class name="dsAlgo_TestClasses.Register_TestClass" />
            <class name="dsAlgo_TestClasses.Stack_TestClass"/>
        </classes>
    </test>
    <test name="Firefox Test">
        <parameter name="browserType" value="Firefox" />
        <classes>
             <class name="dsAlgo_TestClasses.Array_TestClass" />
            <class name="dsAlgo_TestClasses.Tree_TestClass"/>
            <class name="dsAlgo_TestClasses.DataStructure_TestClass" />
            <class name="dsAlgo_TestClasses.Graph_TestClass"/>
            <class name="dsAlgo_TestClasses.Home_TestClass" />
            <class name="dsAlgo_TestClasses.LinkedList_TestClass"/>
            <class name="dsAlgo_TestClasses.Login_TestClass" />
            <class name="dsAlgo_TestClasses.Queue_TestClass"/>
            <class name="dsAlgo_TestClasses.Register_TestClass" />
            <class name="dsAlgo_TestClasses.Stack_TestClass"/>
        </classes>
    </test> 
</suite>
